<div id="app">
  {{ message }}
</div>

<script type="module">
  import { 
    ref,
    createApp
  } from "https://unpkg.com/vue@3.2.36/dist/vue.esm-browser.prod.js"
  
  const app = createApp({
    setup() {
      const message = ref("Hello World")
      return {
      message
      }
    },
  })
  app.mount("#app");
</script>
